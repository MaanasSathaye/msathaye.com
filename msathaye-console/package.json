{
  "name": "msathaye-console",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint  --fix src/**/*.{ts, tsx}",
